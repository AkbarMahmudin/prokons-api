{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "strict": "off",
        "no-unused-vars": "warn",
        "consistent-return": "off",
        "no-param-reassign": "off"
    }
}
